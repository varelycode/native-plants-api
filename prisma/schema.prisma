generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model plants {
  id                                Int     @id @default(autoincrement())
  symbol                            String? @db.VarChar
  accepted_symbol_x                 String? @db.VarChar
  synonym_symbol_x                  String? @db.VarChar
  scientific_name_x                 String? @db.VarChar
  hybrid_genus_indicator            String? @db.VarChar
  hybrid_species_indicator          String? @db.VarChar
  species                           String? @db.VarChar
  subspecies_prefix                 String? @db.VarChar
  hybrid_subspecies_indicator       String? @db.VarChar
  subspecies                        String? @db.VarChar
  variety_prefix                    String? @db.VarChar
  hybrid_variety_indicator          String? @db.VarChar
  variety                           String? @db.VarChar
  subvariety_prefix                 String? @db.VarChar
  subvariety                        String? @db.VarChar
  forma_prefix                      String? @db.VarChar
  forma                             String? @db.VarChar
  genera_binomial_author            String? @db.VarChar
  trinomial_author                  String? @db.VarChar
  quadranomial_author               String? @db.VarChar
  questionable_taxon_indicator      String? @db.VarChar
  parents                           String? @db.VarChar
  common_name                       String? @db.VarChar
  state                             String? @db.VarChar
  category                          String? @db.VarChar
  family                            String? @db.VarChar
  family_symbol                     String? @db.VarChar
  family_common_name                String? @db.VarChar
  xorder                            String? @db.VarChar
  subclass                          String? @db.VarChar
  class                             String? @db.VarChar
  subdivision                       String? @db.VarChar
